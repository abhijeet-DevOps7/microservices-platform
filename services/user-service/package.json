{
  "name": "user-service",
  "version": "1.0.0",
  "description": "User management microservice with authentication and profile management",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "nodemon --exec ts-node src/index.ts",
    "build": "tsc",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "migration:generate": "typeorm migration:generate",
    "migration:run": "typeorm migration:run",
    "migration:revert": "typeorm migration:revert",
    "seed": "ts-node src/database/seeds/index.ts",
    "docker:build": "docker build -t abhijeet-devops7/user-service:latest .",
    "docker:push": "docker push abhijeet-devops7/user-service:latest"
  },
  "keywords": [
    "microservices",
    "user-management",
    "authentication",
    "jwt",
    "nodejs",
    "typescript",
    "postgresql",
    "redis"
  ],
  "author": {
    "name": "Abhijeet DevOps",
    "email": "abhijeet.devops7@example.com",
    "url": "https://github.com/abhijeet-DevOps7"
  },
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/abhijeet-DevOps7/microservices-platform.git"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "compression": "^1.7.4",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "joi": "^17.11.0",
    "pg": "^8.11.3",
    "typeorm": "^0.3.17",
    "redis": "^4.6.10",
    "amqplib": "^0.10.3",
    "dotenv": "^16.3.1",
    "winston": "^3.11.0",
    "express-rate-limit": "^7.1.5",
    "prom-client": "^15.1.0",
    "jaeger-client": "^3.19.0",
    "opentracing": "^0.14.7",
    "express-validator": "^7.0.1",
    "swagger-ui-express": "^5.0.0",
    "swagger-jsdoc": "^6.2.8"
  },
  "devDependencies": {
    "@types/node": "^20.8.7",
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.16",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/compression": "^1.7.5",
    "@types/amqplib": "^0.10.4",
    "@types/swagger-ui-express": "^4.1.6",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/jest": "^29.5.7",
    "@types/supertest": "^2.0.16",
    "typescript": "^5.2.2",
    "ts-node": "^10.9.1",
    "nodemon": "^3.0.1",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "supertest": "^6.3.3",
    "eslint": "^8.52.0",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "prettier": "^3.0.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}